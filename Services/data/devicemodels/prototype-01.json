{
    "SchemaVersion": "1.0.0",
    "Id": "prototype-01",
    "Version": "0.0.1",
    "Name": "Prototyping Device",
    "Description": "Prototyping device with temperature, pressure and GPS.",
    "Protocol": "MQTT",
    "Simulation": {
        "InitialState": {
            "temperature": 65.0,
            "temperature_unit": "F",
            "pressure": 150.0,
            "pressure_unit": "psig",
            "latitude": 47.675738,
            "longitude": -122.209277,
            "moving": false
        },
        "Script": {
            "Type": "javascript",
            "Path": "prototype-01-state.js",
            "Interval": "00:00:05"
        }
    },
    "Properties": {
        "Type": "Prototyping",
        "Firmware": "2.1",
        "Model": "PR002",
        "Location": "Field",
        "Latitude": 47.612514,
        "Longitude": -122.204184
    },
    "Tags": {
        "Location": "Field",
        "Mode": "Test 1"
    },
    "Telemetry": [
        {
            "Interval": "00:00:10",
            "MessageTemplate": "{\"temperature\":${temperature},\"temperature_unit\":\"${temperature_unit}\",\"pressure\":${pressure},\"pressure_unit\":\"${pressure_unit}\",\"latitude\":${latitude},\"longitude\":${longitude}}",
            "MessageSchema": {
                "Name": "prototype-sensors;v1",
                "Format": "JSON",
                "Fields": {
                    "temperature": "double",
                    "temperature_unit": "text",
                    "pressure": "double",
                    "pressure_unit": "text",
                    "latitude": "double",
                    "longitude": "double"
                }
            }
        }
    ],
    "CloudToDeviceMethods": {
        "Reboot": {
            "Type": "javascript",
            "Path": "Reboot-method.js"
        },
        "FirmwareUpdate": {
          "Type": "javascript",
          "Path": "FirmwareUpdate-method.js"
        },
        "StartMoving": {
            "Type": "javascript",
            "Path": "TBD.js"
        },
        "StopMoving": {
            "Type": "javascript",
            "Path": "TBD.js"
        },
        "DecreaseTemperature": {
            "Type": "javascript",
            "Path": "TBD.js"
        },
        "IncreaseTemperature": {
            "Type": "javascript",
            "Path": "TBD.js"
        }
    }
}
